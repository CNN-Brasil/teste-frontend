{
	"compilerOptions": {
		"allowJs": true,
		"target": "es5",
		"module": "commonjs",
		"jsx": "react",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist"
	},
	"include": ["./src/**/*"],
	"exclude": ["**/*.spec.ts", "src/**/*.ts", "**/*.d.ts", "node_modules"]
}
